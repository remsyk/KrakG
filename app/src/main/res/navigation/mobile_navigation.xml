<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_bots">

    <fragment
        android:id="@+id/navigation_bots"
        android:name="com.example.krakg.ui.fragments.BotsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_bots" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.krakg.ui.fragments.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.krakg.ui.fragments.SettingsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_settings" />
</navigation>